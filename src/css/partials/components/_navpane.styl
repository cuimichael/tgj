.navpane
  width: 100%
  height: 100%
  background-color: hsla(0deg, 0% 97%, 1)

  .navpane-nav
    ul
      list-style: none
      margin: 0
      padding-left: 0
      li
        display: block

.navpane-nav ul li a
  display: block
  font-size: 18px
  padding: 10px 15px
  &:hover, &:focus
    text-decoration: none
    background-color: #eee

.nav-divider 
  height: 1px
  overflow: hidden
  margin: 9px 0
  background-color: #e5e5e5

.brand
  position: absolute
  bottom: 0
  width: 100%
  hr
    margin: 0
  .logo
    padding: 15px
